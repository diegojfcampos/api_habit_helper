{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.ts"
      }
    ],
    "env": {
      "TS_NODE_TRANSPILE_ONLY": "true"
    },
    "scripts": {
      "build": "npx tsc"
    },
    "installConfig": {
      "include": [
        "src/**"
      ]
    }
  }
  